# Agent Explorer Environment Variables
# Copy this file to .env.local and fill in your values

# ===========================================
# Required: RPC URLs for blockchain networks
# ===========================================

# Ethereum Sepolia (Chain ID: 11155111)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Base Sepolia (Chain ID: 84532)
BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Polygon Amoy (Chain ID: 80002)
POLYGON_AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# ===========================================
# Optional: Application Configuration
# ===========================================

# Public URL for the application (used for meta tags)
NEXT_PUBLIC_APP_URL=http://localhost:3000

